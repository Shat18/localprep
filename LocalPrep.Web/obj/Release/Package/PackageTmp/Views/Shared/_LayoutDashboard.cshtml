<script>
    $(document).ready(function () {


        $('#btnCustomerFilter').on("click", function () {
           /* alert($(".first_name").val());*/
            $(".loader-container").show();
            
            
            
            /*var form = jQuery("#frmBecomePrepper");*/
            var status = form.valid();
            if (status) {
                //$(".first_name").val("");
                //$(".form_date").val("");
                $(".loader-container").hide();
            }
        });

        $("li.customers a").addClass('active');

        $(".view-detail").click(function () {
           /* alert(1);*/
            $('.recent-order').hide();
            $('.order-deail').slideToggle();
        });

        $(".view-detail-History").click(function () {
            /*alert(2);*/
            $('.History-order').hide();
            $('.order-deail-History').slideToggle();
        });

        $(".backback-list-History").click(function () {
            $('.History-order').hide();
            $('.order-deail-History').slideToggle();

        });
        
        

        $(".back-list").click(function () {
            $('.order-deail').hide();
            $('.recent-order').slideToggle();

        });

        $(".view-detail-past").click(function () {

            $('.recent-order-past').hide();
            $('.order-deail-past').slideToggle();
        });
        $(".back-list-past").click(function () {
            $('.order-deail-past').hide();
            $('.recent-order-past').slideToggle();

        });

        $.getJSON("/Admin/GetData", function (data) {
            var Sells = []
            var Months = []

            for (var i = 0; i < data.totalsell.length; i++) {
                Sells.push(data.totalsell[i].totalSell);
            }
            for (var i = 0; i < data.monthname.length; i++) {
                Months.push(data.monthname[i].monthName);
            }

            Highcharts.chart('container', {

                title: {
                    text: 'Total Sales'
                },
                chart: {
                    type: 'line'
                },

                xAxis: {
                    categories: Months
                },
                yAxis: {
                    tickPositions: [2000, 4000, 6000, 8000, 10000, 12000, 14000, 16000, 18000, 200000, 22000, 24000],
                },

                series: [{

                    data: Sells
                }]
            });
        });


    })
</script>